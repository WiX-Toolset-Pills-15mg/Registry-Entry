<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
    <Fragment>

        <Binary
			Id="CustomActionsBinaryFile"
			SourceFile="$(var.RegistryEntry.CustomActions.TargetDir)$(var.RegistryEntry.CustomActions.TargetName).CA.dll"/>

        <CustomAction
			Id="IncrementRunCount"
			BinaryKey="CustomActionsBinaryFile"
			DllEntry="IncrementRunCount"
			Execute="immediate"
			Return="check"/>

    </Fragment>
</Wix>
